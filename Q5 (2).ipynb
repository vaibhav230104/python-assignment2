   "source": [
    "def prime_generator():\n",
    "\n",
    "    n = 2\n",
    "    while n < 1000:\n",
    "        if all(n % i != 0 for i in range(2, int(n**0.5) + 1)):\n",
    "            yield n\n",
    "        n += 1\n",
    "\n",
    "prime_gen = prime_generator()\n",
    "for i in range(20):\n",
    "    print(next(prime_gen))\n",
    "    "
