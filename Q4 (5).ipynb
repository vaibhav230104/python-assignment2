   "source": [
    "#Generator function is similar to normal function but it uses yield for returning values so if \n",
    "def count_test(n):\n",
    "    count = 1 \n",
    "    while count <= n :\n",
    "        yield count\n",
    "        count = count + 1\n",
    "\n",
    "for i in count_test(5):\n",
    "    print(i)"
   ]
