   ],
   "source": [
    "i = int(input(\"Enter the number\"))\n",
    "x=i\n",
    "rev = 0\n",
    "while i>0:\n",
    "    rev = (rev)*10 + i%10\n",
    "    i=i//10\n",
    "if (x == rev):\n",
    "    print(\"The number is palindrome\")\n",
    "else:\n",
    "    print(\"The number is not palindrome\")\n"
   ]
